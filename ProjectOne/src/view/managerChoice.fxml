<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="93.0" prefWidth="341.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="control.ManagerChoiceControl">
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<HBox alignment="CENTER" layoutX="27.0" layoutY="22.0"
		prefHeight="87.0" prefWidth="365.0" spacing="30.0">
		<children>
			<Button fx:id="btnApplicant" prefHeight="53.0"
				prefWidth="92.0" style="-fx-background-color: white;">
				<graphic>
					<ImageView fitHeight="100" fitWidth="200"
						pickOnBounds="true" preserveRatio="true">
						<image>
							<Image url="/image/applicant.jpg" />
						</image>
					</ImageView>
				</graphic>
			</Button>
			<Button fx:id="btnEmployee" prefHeight="54.0" prefWidth="98.0"
				style="-fx-background-color: white;">
				<graphic>
					<ImageView fitHeight="100" fitWidth="200"
						pickOnBounds="true" preserveRatio="true">
						<image>
							<Image url="/image/employee.jpg" />
						</image>
					</ImageView>
				</graphic>
			</Button>
			<Button fx:id="btnCancel" mnemonicParsing="false"
				prefHeight="90.0" prefWidth="100.0"
				style="-fx-background-color: transparent;">
				<graphic>
					<ImageView fitHeight="90" fitWidth="90"
						pickOnBounds="true" preserveRatio="true">
						<image>
							<Image url="/image/LogOut.png" />
						</image>
					</ImageView>
				</graphic>
			</Button>
		</children>
	</HBox>
	<effect>
		<DropShadow height="80.3" radius="30.755" spread="0.07"
			width="44.72" />
	</effect>
</AnchorPane>
